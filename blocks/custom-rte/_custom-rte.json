{
  "definitions": [
    {
      "title": "Custom  RTE",
      "id": "custom-rte",
      "plugins": {
        "xwalk": {
          "page": {
            "resourceType": "core/franklin/components/text/v1/text",
            "template": {
              "name": "Custom RTE"
            }
          }
        }
      }
    }
  ],
  "models": [],
  "filters": [
    {
      "id": "custom-rte",

      "rte": {
        "format": [
          "bold",
          "italic",
          "underline",
          "strikethrough",
          "font",
          "fontsize",
          "color",
          "background",
          "uppercase",
          "lowercase",
          "capitalize"
        ],
        "alignment": ["left", "center", "right", "justify"],
        "indentation": ["indent", "outdent", "lineHeight", "letterSpacing"],
        "sr_script": ["subscript", "superscript"],
        "list": ["unordered", "ordered", "checklist"],
        "insert": [
          "link",
          "unlink",
          "anchor",
          "image",
          "video",
          "audio",
          "table",
          "hr",
          "datetime",
          "specialChar",
          "emoji",
          "math",
          "equation",
          "toc",
          "footnote"
        ],
        "editor": [
          "undo",
          "redo",
          "removeformat",
          "selectall",
          "sourcecode",
          "preview",
          "fullscreen",
          "spellcheck",
          "wordcount",
          "findreplace"
        ],
        "advanced": [
          "comments",
          "mentions",
          "revisionHistory",
          "accessibilityChecker",
          "complianceChecker",
          "mergeTags",
          "aiAssistant",
          "imageEditor",
          "powerPaste",
          "imageOptimizer"
        ],
        "extensions": [
          "customCSS",
          "customTags",
          "downloadableFiles",
          "pluginSupport"
        ],
        "ui": {
          "language": "en",
          "theme": "light",
          "toolbarLayout": "top",
          "showStatusbar": true
        }
      }
    }
  ]
}
